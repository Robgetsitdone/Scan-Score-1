{
  "expo": {
    "name": "Score the Label",
    "slug": "scan-and-score",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "scanandscore",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F8F9FA"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.scanandscore",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "Score the Label needs camera access to scan food labels and barcodes so you can analyze ingredients.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.scanandscore",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#F8F9FA",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://replit.com/"
        }
      ],
      "expo-font",
      "expo-web-browser",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Score the Label to access your camera to scan food labels and barcodes."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": ""
      }
    },
    "owner": "robgetsitdone"
  }
}
